{
  "name": "@agentgov/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "(prisma migrate deploy || prisma db push) && node dist/index.js",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@fastify/websocket": "^11.2.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sentry/node": "^10.38.0",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.4.18",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.4",
    "fastify-plugin": "^5.1.0",
    "fastify-raw-body": "^5.0.0",
    "ioredis": "^5.3.2",
    "node-cron": "^4.2.1",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "prom-client": "^15.1.0",
    "resend": "^6.9.1",
    "stripe": "^20.3.0",
    "zod": "^4.3.6",
    "@agentgov/shared": "workspace:*"
  },
  "devDependencies": {
    "@agentgov/eslint-config": "workspace:*",
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^7.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
